for s in /opt/internal/dev/utils/profile.d/*.sh; do
    if [ -r "$s" ]
    then
        . $s
    else
        echo "Could not load $s"
        echo "please confirm with a system administrator this server has been configured correctly."
    fi
done

export PATH=$PATH:/opt/internal/dev/bin:/apollo/env/SDETools/bin:/apollo/env/WebDevTools/bin
export LF_USER=robtay
export PROJECT=robtay-amz
export PERL5LIB=$PERL5LIB:/workplace/$PROJECT/src/opt
export ANY_MOOSE=Moose
